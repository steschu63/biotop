<?xml version="1.0"?>
<rdf:RDF
    xmlns:protege-dc="http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:ro="http://www.ifomis.org/obo/ro/1.0#"
    xmlns:snap="http://www.ifomis.org/bfo/1.0/snap#"
    xmlns:bfo="http://www.ifomis.org/bfo/1.0#"
    xmlns:span="http://www.ifomis.org/bfo/1.0/span#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns="http://www.ifomis.org/biotop/1.0#"
    xmlns:p1="http://www.ifomis.org/obo/ro/1.0/light#"
  xml:base="http://www.ifomis.org/biotop/1.0">
  <owl:Ontology rdf:about="">
    <dc:publisher rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >University Hospital Freiburg - Medical Informatics Department</dc:publisher>
    <dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.ifomis.org/biotop/1.0</dc:identifier>
    <dc:publisher rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Jena University Language and Information Engineering (JULIE) Lab</dc:publisher>
    <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Holger Stenzhorn</dc:creator>
    <dc:publisher rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Institute for Formal Ontology and Medical Information Science (IFOMIS)</dc:publisher>
    <dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Stefan Schulz, Elena Beisswanger, Joachim Wermter and Udo Hahn: "Towards an Upper Level Ontology for Molecular Biology"</dc:source>
    <dc:language rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >en</dc:language>
    <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Stefan Schulz</dc:creator>
    <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Elena Beisswanger</dc:creator>
    <dc:format rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >application/rdf+xml</dc:format>
    <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
    >2007-02-14</dc:date>
    <owl:imports rdf:resource="http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl"/>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1.0</owl:versionInfo>
    <dc:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >BioTop</dc:title>
    <dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Stefan Schulz, Elena Beisswanger, Udo Hahn, Joachim Wermter, Holger Stenzhorn and Anand Kumar: "From GENIA to BioTop - Towards a top-level Ontology for Biology"</dc:source>
    <owl:imports rdf:resource="http://www.ifomis.org/obo/ro/1.0/light"/>
    <owl:imports rdf:resource="http://www.ifomis.org/bfo/1.0"/>
  </owl:Ontology>
  <owl:Class rdf:ID="AliphaticChain">
    <owl:disjointWith>
      <owl:Class rdf:ID="AromaticHydrocarbon"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="CyclicAliphatic"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Hydrocarbon"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Definition: Hydrocarbon without rings.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="CellularComponent">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Definition: Components which only can be formed inside living cells but which may survive their host cell.</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="Organism"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Cell"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:someValuesFrom>
              <owl:Class rdf:about="#Organism"/>
            </owl:someValuesFrom>
            <owl:onProperty>
              <owl:TransitiveProperty rdf:about="http://www.ifomis.org/obo/ro/1.0/light#derivesFrom"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:TransitiveProperty rdf:about="http://www.ifomis.org/obo/ro/1.0/light#properPartOf"/>
            </owl:onProperty>
            <owl:someValuesFrom>
              <owl:Class rdf:about="#Organism"/>
            </owl:someValuesFrom>
          </owl:Restriction>
        </owl:unionOf>
      </owl:Class>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="PolyMolecularCompound"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Phosphate">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:allValuesFrom>
              <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                  <owl:Class rdf:ID="HydrogenAtom"/>
                  <owl:Class rdf:ID="OxygenAtom"/>
                  <owl:Class rdf:ID="PhosphorusAtom"/>
                </owl:unionOf>
              </owl:Class>
            </owl:allValuesFrom>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="hasComponent"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#hasComponent"/>
            </owl:onProperty>
            <owl:someValuesFrom>
              <owl:Class rdf:about="#OxygenAtom"/>
            </owl:someValuesFrom>
          </owl:Restriction>
          <owl:Restriction>
            <owl:someValuesFrom>
              <owl:Class rdf:about="#PhosphorusAtom"/>
            </owl:someValuesFrom>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#hasComponent"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Class rdf:ID="MolecularEntity"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Definition: Phosphate is a salt of phosphoric acid or a functional group on organic compounds.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="ProkaryoticCell">
    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:ID="BacterialCell"/>
          <owl:Class rdf:ID="ArchaebacterialCell"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Comment: We conceive organisms in a material sense. Therefore we do not assume any ontological difference between a monocellular organism and the cell it is made of.</rdfs:comment>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Synonym: Prokaryote</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="UniCellularOrganism"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Eukaryote"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="YeastCell"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="C5Chain">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Comment: (QCR) hasComponent exactly 5 CarbonAtom</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="CarbonHydrogenHydroxySubstructure"/>
    </owl:disjointWith>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Definition: Backbone of Pentoses.</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="CarboxyGroup"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="HydroxyGroup"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="AminoGroup"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="C6Chain"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="CarbonHydrogenSubstructure"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="CarbonChain"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Hexose">
    <owl:disjointWith>
      <owl:Class rdf:ID="Pentose"/>
    </owl:disjointWith>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Comment: (QCR) hasComponent exactly 1 C6Chain</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:TransitiveProperty rdf:about="http://www.ifomis.org/obo/ro/1.0/light#hasProperPart"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#HydroxyGroup"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#CarbonHydrogenSubstructure"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:TransitiveProperty rdf:about="http://www.ifomis.org/obo/ro/1.0/light#hasProperPart"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Monosaccharide"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="NonStreoidLipid">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:TransitiveProperty rdf:about="http://www.ifomis.org/obo/ro/1.0/light#hasProperPart"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="AliphaticMediumChainStructure"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Lipid"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Steroid"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="EntireProteinMolecule">
    <owl:disjointWith>
      <owl:Class rdf:ID="EntirePolypeptideMolecule"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="EntireOligopeptideMolecule"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Peptide"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Comment: (QCR) Peptide and hasComponent min 101 AminoAcidMonomer and not (ro:properPartOf some MolecularEntity)</rdfs:comment>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Definition: Amino Acid Polymer longer than 100 monomers.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Cytosine">
    <owl:disjointWith>
      <owl:Class rdf:ID="Uracil"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Thymin"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Guanine"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Adenine"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="HeterocyclicBase"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Glucose">
    <rdfs:subClassOf rdf:resource="#Hexose"/>
  </owl:Class>
  <owl:Class rdf:ID="EntireMolecularEntity">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Definition: Separately distinguishable molecular structure, neither part of nor attached to any other molecular structure. Atoms are not considered molecular structures. The hierarchy of entire entities mirrors the hierarchy of structures.</rdfs:comment>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#MolecularEntity"/>
          <owl:Class>
            <owl:complementOf>
              <owl:Restriction>
                <owl:onProperty>
                  <owl:TransitiveProperty rdf:about="http://www.ifomis.org/obo/ro/1.0/light#properPartOf"/>
                </owl:onProperty>
                <owl:someValuesFrom>
                  <owl:Class rdf:about="#MolecularEntity"/>
                </owl:someValuesFrom>
              </owl:Restriction>
            </owl:complementOf>
          </owl:Class>
          <owl:Class rdf:about="http://www.ifomis.org/bfo/1.0/snap#Object"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <owl:disjointWith>
      <owl:Class rdf:ID="MolecularGroup"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="Fungus">
    <owl:disjointWith>
      <owl:Class rdf:ID="Protist"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Plant"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Animal"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Eukaryote"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Tissue">
    <rdfs:subClassOf>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:someValuesFrom>
              <owl:Class rdf:about="#Organism"/>
            </owl:someValuesFrom>
            <owl:onProperty>
              <owl:TransitiveProperty rdf:about="http://www.ifomis.org/obo/ro/1.0/light#derivesFrom"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty rdf:resource="http://www.ifomis.org/obo/ro/1.0/light#partOf"/>
            <owl:someValuesFrom>
              <owl:Class rdf:about="#Organism"/>
            </owl:someValuesFrom>
          </owl:Restriction>
        </owl:unionOf>
      </owl:Class>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasComponent"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="hasGrain"/>
            </owl:onProperty>
            <owl:someValuesFrom>
              <owl:Class rdf:about="#Cell"/>
            </owl:someValuesFrom>
          </owl:Restriction>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasComponent"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#hasGrain"/>
            </owl:onProperty>
            <owl:someValuesFrom rdf:resource="#EntireMolecularEntity"/>
          </owl:Restriction>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="PortionOfHeterogenousSolid"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Definition: Aggregate of an arbitrary number of congeneric cells (cells with identical specialized characteristics), embedded into an amount of matter (matrix) that work together to perform a specific function.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="MulticellularOrganism">
    <owl:disjointWith>
      <owl:Class rdf:ID="Virus"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#UniCellularOrganism"/>
    </owl:disjointWith>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Organism"/>
          <owl:Restriction>
            <owl:onProperty>
              <owl:TransitiveProperty rdf:about="http://www.ifomis.org/obo/ro/1.0/light#hasProperPart"/>
            </owl:onProperty>
            <owl:someValuesFrom>
              <owl:Class rdf:about="#Cell"/>
            </owl:someValuesFrom>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="ModifiedCell">
    <rdfs:subClassOf>
      <owl:Class>
        <owl:complementOf>
          <owl:Restriction>
            <owl:someValuesFrom>
              <owl:Class rdf:about="#Organism"/>
            </owl:someValuesFrom>
            <owl:onProperty>
              <owl:TransitiveProperty rdf:about="http://www.ifomis.org/obo/ro/1.0/light#properPartOf"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:complementOf>
      </owl:Class>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:TransitiveProperty rdf:about="http://www.ifomis.org/obo/ro/1.0/light#derivesFrom"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="CellInVivo"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Cell"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Definition: Cells that grow and replicate continuously outside the living organism.</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="#Animal"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#CellInVivo"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="PhysicalAnatomicalEntity">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Superclass of  FMA physical anatomical entity since it contains also non-canonical anatomical entities and is not restricted to human. Has spatial dimension(s).</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="ObjectOrFiatObjectPart"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="PortionOfHomogenousLiquid">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="PortionOfHomogenousMatter"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Pure liquid consisting only of one kind of particles, e.g. H2O, liquid Hg, ...</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Cytoplasm">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#hasGrain"/>
            </owl:onProperty>
            <owl:someValuesFrom>
              <owl:Class rdf:ID="WaterMolecule"/>
            </owl:someValuesFrom>
          </owl:Restriction>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="http://www.ifomis.org/obo/ro/1.0/light#hasPart"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#CellularComponent"/>
  </owl:Class>
  <owl:Class rdf:about="#Thymin">
    <owl:disjointWith>
      <owl:Class rdf:about="#Uracil"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Guanine"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Cytosine"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Adenine"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#HeterocyclicBase"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="NitrogenAtom">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Atom"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="CarbonAtom"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#OxygenAtom"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#HydrogenAtom"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#PhosphorusAtom"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="SulphurAtom"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="PortionOfHeterogenousMatter">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="PortionOfCohesiveMatter"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasComponent"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="CollectiveObject"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#CollectiveObject"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasComponent"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="#PortionOfHomogenousMatter"/>
    </owl:disjointWith>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Composed by components of different kinds of Collectives. These "collective components" may be cohesive (e.g. a solvent) of disconnected (e.g. the solute) in a chemical solution.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#EntireOligopeptideMolecule">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#hasComponent"/>
            </owl:onProperty>
            <owl:allValuesFrom>
              <owl:Class rdf:ID="AminoAcidMonomer"/>
            </owl:allValuesFrom>
          </owl:Restriction>
          <owl:Restriction>
            <owl:someValuesFrom>
              <owl:Class rdf:about="#AminoAcidMonomer"/>
            </owl:someValuesFrom>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#hasComponent"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Class rdf:about="#Peptide"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <owl:disjointWith rdf:resource="#EntireProteinMolecule"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#EntirePolypeptideMolecule"/>
    </owl:disjointWith>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Comment: (QCR) hasComponent max 10 AminoAcidMonomer</rdfs:comment>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Definition: Amino Acid Polymer up to 10 monomers.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="EntireDNAMolecule">
    <owl:equivalentClass>
